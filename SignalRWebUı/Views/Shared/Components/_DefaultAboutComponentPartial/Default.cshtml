@model List<SignalRWebUı.Dtos.AboutDtos.ResultAboutDto>
@foreach(var x in Model)
{
    <section class="about_section layout_padding">
        <div class="container  ">

            <div class="row">
                <div class="col-md-6 ">
                    <div class="img-box">
                        <img src="@x.AboutImageUrl" alt="">
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="detail-box">
                        <div class="heading_container">
                            <h2>
                                @x.AboutTitle
                            </h2>
                        </div>
                        <p>
                            @x.AboutDescription
                        </p>
                        <a href="">
                            Devamını Oku
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>
}
